{
  "consumer": {
    "name": "OrganizationServiceConsumer"
  },
  "interactions": [
    {
      "description": "a request to confirm invitation",
      "providerState": "Organization exists",
      "request": {
        "body": {
          "firstName": "John",
          "lastName": "Doe"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.firstName": {
            "match": "type"
          },
          "$.body.lastName": {
            "match": "type"
          },
          "$.path": {
            "match": "regex",
            "regex": "/organization/join/[0-9]+"
          }
        },
        "method": "POST",
        "path": "/organization/join/1"
      },
      "response": {
        "status": 201
      }
    },
    {
      "description": "a request to invite a member",
      "providerState": "Organization organization-name exists",
      "request": {
        "body": {
          "email": "test@example.com"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.email": {
            "match": "type"
          }
        },
        "method": "POST",
        "path": "/organization/organization-name/member"
      },
      "response": {
        "status": 202
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "15.0.1"
    },
    "pactRust": {
      "ffi": "0.4.22",
      "models": "1.2.3"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "OrganizationServiceProvider"
  }
}